{
          "Command": "/usr/bin/python3 -m http.server 9090",
          "Endpoints": [{
              "Name": "websvc",
              "Application": "websvc",
              "PortNumber": 9090,
              "Protocol": "tcp",
              "Purpose": "export",
              "VHosts": ["websvc"]
            }
          ],
          "ConfigFiles": {
                "/etc/bar.txt": {
                    "Filename": "/etc/bar.txt",
                    "Owner": "root:root",
                    "Permissions": "660"
                }
          },
          "ImageID": "zenoss/ubuntu:wget",
          "Instances": {
            "min": 1
          },
          "LogConfigs":[ {
            "path": "/tmp/foo",
            "type": "foo"
          }],
          "Runs": {
            "exit0": "date",
            "exit1": "date; exit 42",
            "sleepy60": "sleep 60"
          },
          "Snapshot": {
            "Pause": "echo pause",
            "Resume": "echo resume"
          }
}
